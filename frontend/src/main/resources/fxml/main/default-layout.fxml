<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" prefHeight="800.0" prefWidth="1280.0" style="-fx-background-color: #f5efee;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane>
         <children>
            <VBox fx:id="mainVBoxLayout" style="-fx-background-color: transparent;">
               <children>
                  <GridPane fx:id="topLayout" prefHeight="100.0">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
                  <HBox prefHeight="800.0" prefWidth="1280.0" style="-fx-background-color: transparent;">
                     <children>
                        <StackPane fx:id="leftLayout" prefWidth="350.0" />
                        <GridPane fx:id="bodyLayout" maxWidth="930.0" prefWidth="930.0">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                        </GridPane>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="popUpVBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: transparent;" />
            <HBox fx:id="popUpSmallHBox" alignment="TOP_CENTER" maxHeight="70.0" prefHeight="70.0">
               <StackPane.margin>
                  <Insets bottom="700.0" />
               </StackPane.margin>
            </HBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
