<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane id="shadowPane" prefHeight="700.0" prefWidth="350.0" stylesheets="@../../styles/table.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-background-radius: 0 0 20 20;" />
      <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0" stylesheets="@../../styles/table.css">
         <children>
            <StackPane id="tab_column_blend" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="50.0" maxHeight="40.0" prefHeight="40.0" prefWidth="350.0" stylesheets="@../../styles/table.css">
               <padding>
                  <Insets right="30.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" maxHeight="40.0" prefHeight="40.0" prefWidth="350.0" stylesheets="@../../styles/table.css">
               <padding>
                  <Insets right="30.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </StackPane>
            <StackPane id="tab" fx:id="searchButton" alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="350.0" stylesheets="@../../styles/table.css">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/research-icon.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="searchLabel" stylesheets="@../../styles/table.css" text="%Tab.search" textFill="#333333">
                           <font>
                              <Font name="Candara Bold" size="22.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets right="30.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane id="tab" fx:id="detailsButton" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" prefHeight="60.0" prefWidth="350.0" stylesheets="@../../styles/table.css">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/detail-icon.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="detailLabel" stylesheets="@../../styles/table.css" text="%Tab.detail" textFill="#333333">
                           <font>
                              <Font name="Candara Bold" size="22.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets right="30.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane id="tab" fx:id="importButton" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" prefHeight="60.0" prefWidth="350.0" stylesheets="@../../styles/table.css">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/import-icon.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="importLabel" stylesheets="@../../styles/table.css" text="%Tab.import" textFill="#333333">
                           <font>
                              <Font name="Candara Bold" size="22.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets right="30.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" />
   </padding>
</StackPane>
